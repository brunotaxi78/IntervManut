@model IEnumerable<IntervManut.Models.Tecnico>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoTec)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoTec.TipoTecId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TecnicoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TecnicoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TecnicoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
